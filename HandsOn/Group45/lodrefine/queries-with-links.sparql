

PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT DISTINCT ?s ?predicate ?object ?hastitulo
	
	WHERE {
  		?s rdf:type <https://schema.org/Eventos> .
		?s <https://schema.org/hastitulo> ?hastitulo .
  		
	}

SELECT DISTINCT ?s   ?Localiza
	
	WHERE {
  		?s rdf:type <https://schema.org/Biblioteca>. 
		?s <https://schema.org/hasdireccion> ?Localiza .
  		
	}